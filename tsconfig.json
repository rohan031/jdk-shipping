{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@data/*": [
        "src/data/*"
      ],
      "@scripts/*": [
        "src/scripts/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}